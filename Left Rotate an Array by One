import java.util.* ;
import java.io.*; 


public class Solution {

    static int[] rotateArray(int[] arr, int n) {
        // Write your code here.
        int [] res=new int[n];
        for(int i=1;i<n;i++){
            res[i-1]=arr[i];
        }
        res[n-1]=arr[0];
        return(res);

    }
}
