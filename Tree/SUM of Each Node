public static int sum(TreeNode root){
        int s=0;
        int l=0;
        int r=0;
        if(root == null){
            return 0;
        }
        else{
            if(root.left!=null){
                l=sum(root.left);
            }
             if(root.right!=null){
                r=sum(root.right);
            }
            s=root.val+l+r;
            return s;
        }    
    }
